
<form method="POST" action="/login">
  <div class="container">
    <div class="box">
      <div style="justify-content: center; text-align: center;" class="mt-3">
        <img src="/img/logo.png" alt="Icon" width="40">
        <h2 class="mt-2 mb-0">LOGIN USER</h2>
        <p class="text-muted mb-3">Panitia Pengawas Pemilu</p>
      </div>
      <div class="form-group mt-3">
        <label for="kode_aktivasi">Masukkan Kode Login</label>
        <input id="kode_aktivasi" name="kode_aktivasi" class="form-control">
      </div>
      <div class="mt-3 mb-4">
        <button type="submit" class="custom-button" id="submitButton">Masuk Sekarang</button>
      </div>
    </div>
  </div>
</form>
